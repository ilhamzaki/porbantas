{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "off",
        "no-underscore-dangle": "off",
        "no-empty-function": "off",
        "no-console" : 0,
        "no-plusplus": "off"
    }
}
